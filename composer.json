{
    "name": "vizrex/laraviz",
    "description": "A base package with various helpers and abstract classes, acting as a foundation and a standard for other packages.",
    "type": "laravel-package",
    "license": "proprietary",
    "authors": [{
        "name": "Vizrex",
        "email": "pieter.marchand@yeastconsulting.be"
    }],
    "require": {
        "php": ">=7.0.0",
        "spatie/laravel-enum": "^2.5"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Vizrex\\Laraviz\\": "src/"
        },
        "files": [
            "src/Helpers/StringHelpers.php",
            "src/Helpers/SystemHelpers.php",
            "src/Helpers/EloquentHelpers.php",
            "src/Helpers/HtmlHelpers.php",
            "src/Helpers/PathHelpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vizrex\\Laraviz\\Providers\\LaravizServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
